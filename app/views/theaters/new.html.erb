<h2> Create New Theater</h2>

<%= form_with url: "/theaters", method: :post, local: true do |form|  %>
  <br>
  <%= form.label :name %>
  <%= form.text_field :name %><br>
  <br>
  <%= form.label :open %>
  <%= form.text_field :open %><br>
  <br>
  <%= form.label :review %>
  <%= form.text_field :review%><br>
  <br>
  <%= form.submit "Create Theater" %>
<% end %>